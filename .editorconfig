# http://EditorConfig.org
root = true

[{*.c,Makefile}]
charset = utf-8
end_of_line = lf
indent_style = tab
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
indent_style = space
trim_trailing_whitespace = false

# To ignore an entire subtree: echo 'root = true' > dir/.editorconfig
